{
  "name": "mireya",
  "version": "0.1.0",
  "description": "Backend framework",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./application": {
      "types": "./dist/application/index.d.ts",
      "import": "./dist/application/index.js"
    },
    "./hono": {
      "types": "./dist/hono/index.d.ts",
      "import": "./dist/hono/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    },
    "./module": {
      "types": "./dist/module/index.d.ts",
      "import": "./dist/module/index.js"
    },
    "./service": {
      "types": "./dist/service/index.d.ts",
      "import": "./dist/service/index.js"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "dts-bundle-generator": "^9.3.1",
    "get-tsconfig": "^4.7.2",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "hono": "^4.0.2",
    "reflect-metadata": "^0.2.1",
    "zod": "^3.22.4"
  },
  "keywords": [
    "backend",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/artyom-silantiev/mireya.git"
  },
  "author": "Artyom Silantiev",
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  }
}
